{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2022",
    "lib": ["ESNext", "DOM"],
    "useDefineForClassFields": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "outDir": "out"
  },
  "include": ["src"]
}
